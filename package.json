{
  "name": "year-of-data-visualization",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --check --ignore-path .gitignore .",
    "format:fix": "prettier --write --ignore-path .gitignore .",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "serve": "serve out",
    "generate:video": "CYPRESS_NO_COMMAND_LOG=1 cypress run --e2e --headless --no-runner-ui",
    "generate:video:youtube": "CYPRESS_NO_COMMAND_LOG=1 VIDEO_TYPE=youtube cypress run --e2e --headless --no-runner-ui --browser chrome"
  },
  "dependencies": {
    "@ant-design/charts": "^2.0.1",
    "@kurkle/color": "^0.3.2",
    "@nivo/bar": "^0.84.0",
    "@nivo/boxplot": "^0.84.0",
    "@nivo/bullet": "^0.84.0",
    "@nivo/bump": "^0.84.0",
    "@nivo/calendar": "^0.84.0",
    "@nivo/geo": "^0.84.0",
    "@nivo/heatmap": "^0.84.0",
    "@nivo/line": "^0.84.0",
    "@nivo/pie": "^0.84.0",
    "@nivo/radar": "^0.84.0",
    "@nivo/radial-bar": "^0.84.0",
    "@nivo/sankey": "^0.84.0",
    "@nivo/scatterplot": "^0.84.0",
    "@nivo/stream": "^0.84.0",
    "@nivo/treemap": "^0.84.0",
    "@tanstack/react-table": "^8.13.2",
    "@uidotdev/usehooks": "^2.4.1",
    "@vercel/analytics": "^1.2.0",
    "@vercel/speed-insights": "^1.0.10",
    "antd": "^5.14.2",
    "chart.js": "^4.4.0",
    "dat.gui": "^0.7.9",
    "dayjs": "^1.11.10",
    "next": "14.0.3",
    "react": "^18",
    "react-animated-numbers": "^0.18.0",
    "react-chartjs-2": "^5.2.0",
    "react-charts": "^3.0.0-beta.57",
    "react-dom": "^18",
    "react-select": "^5.8.0",
    "react-super-responsive-table": "^5.2.2",
    "react-tagcloud": "^2.3.3",
    "three": "^0.161.0",
    "vivus": "^0.4.6"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "autoprefixer": "^10.0.1",
    "cypress": "^13.6.4",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "postcss": "^8",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "serve": "^14.2.1",
    "tailwindcss": "^3.4.1"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
